using System;


namespace Amstrong
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("enter a Numer");
            int num = int.Parse(Console.ReadLine());
            int r, sum = 0;
            int temp = num;
            while(num>0)
            {
                r = num % 10;
                sum = sum + (r * r * r);
                num = num / 10;

            }
            if (temp == num)
            {
                Console.WriteLine(num + " it is Armstorng number");
            }
            else
            {
                Console.WriteLine(num + " it  not a armstorng number");
            }

        }
    }
}
